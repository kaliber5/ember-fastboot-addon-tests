{
  "name": "ember-fastboot-addon-tests",
  "version": "0.5.1",
  "description": "FastBoot testing support for Ember addons",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app blueprints config lib server test-support tests node-tests fastboot-tests",
    "start": "ember serve",
    "test:all": "ember try:each",
    "fastboottest": "DEBUG=ember-fastboot-addon-tests,ember-cli-addon-tests ember fastboot:test",
    "nodetest": "mocha node-tests --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaliber5/ember-fastboot-addon-tests"
  },
  "engines": {
    "node": "6.* || >= 8.*"
  },
  "author": "Simon Ihmig <ihmig@kaliber5.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.12.10",
    "broccoli-asset-rev": "^2.4.5",
    "chai": "^3.5.0",
    "ember-cli": "~3.1.4",
    "ember-cli-blueprint-test-helpers": "^0.17.2",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^1.1.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "1.0.0-beta.3",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.2.3"
  },
  "keywords": [
    "ember-addon",
    "fastboot",
    "tests"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "core-object": "^3.1.5",
    "debug": "^3.1.0",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-router-generator": "^1.2.2",
    "fs-extra": "^6.0.0",
    "jquery": "^3.3.1",
    "jsdom": "^16.5.0",
    "mocha": "^5.1.1",
    "request": "^2.85.0",
    "rsvp": "^4.8.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
